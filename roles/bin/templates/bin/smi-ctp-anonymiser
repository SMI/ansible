#!/usr/bin/env python3
# {{ ansible_managed }}
import os

import _smi_wrapper as wrapper

args, remaining_argv, env, config_dir = wrapper.init()
cmd = (
    "java",
    *("-jar", os.path.join(env["SMI_SMISERVICES_INSTALL_DIR"], "CTPAnonymiser.jar")),
    *("-y", os.environ["SMI_SMISERVICES_EXTRACT_PIPELINE_CONFIG"]),
    *("-a", os.environ["SMI_SMISERVICES_CTP_ALLOWLIST"]),
)
wrapper.run(args, remaining_argv, cmd, env)
